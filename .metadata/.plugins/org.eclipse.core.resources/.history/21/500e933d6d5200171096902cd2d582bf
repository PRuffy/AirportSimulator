/** 
 * 
 */
package framework.environment

import java.util.ArrayList

/** 
 * @author stay
 * 
 */
class Graph {
	var nodeList : ArrayList<GraphNode>
	var edgeList : ArrayList<GraphEdge>


	
	def addNode(node : GraphNode){
		nodeList.add(node)
	}
	
	def addEdge(edge : GraphEdge){
		edgeList.add(edge)
	}
	
}
